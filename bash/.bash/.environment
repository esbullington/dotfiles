OPT=$HOME/opt

# For some odd reason, system locale not set for local user
export LANG=en_US.UTF-8
export LC_ALL=en_US.UTF-8
export LANGUAGE=en_US.UTF-8
export EDITOR=vi
export ALPHAVANTAGE=KOYWJXLIT4BXPUM9

# Git editor
export GIT_EDITOR=vim

# DOCKER
export DOCKER_CONTENT_TRUST=1

# Webassembly
export PATH=$PATH:$HOME/repos/wabt/out/clang/Debug

# Cling
export PATH=$PATH:$HOME/opt/cling_2017-04-15_ubuntu16/bin

# nim
export PATH=$PATH:$HOME/opt/nim-0.17.0/bin

# hashcat
export PATH=$PATH:$HOME/opt/hashcat-3.6.0

#flow
export PATH=$PATH:$HOME/opt/flow

#flow
export PATH=$PATH:$HOME/opt/q/l32
export QHOME=$HOME/opt/q

# nvm
export NVM_DIR="/home/eric/.nvm"
[ -s "$NVM_DIR/nvm.sh" ] && . "$NVM_DIR/nvm.sh"  # This loads nvm
# yarn 
export PATH=$PATH:$HOME/.config/yarn/global/node_modules/.bin

# Hash slinger
export PATH=$PATH:$HOME/opt/hash-slinger-2.7

# Cabal
export PATH=$PATH:$HOME/.cabal/bin/

# OCaml
# eval `opam config env`

# Go
export GOPATH=$HOME/projects/go

# Swift
export PATH=$PATH:$HOME/opt/swift/swift-4.0-RELEASE-ubuntu16.04/usr/bin

# Ada
ADACORE=$OPT/adacore
export PATH=$PATH:$ADACORE/gnat/bin:$ADACORE/gprbuild/bin:$ADACORE/spark2014/bin

# Digital Ocean
source <(/home/eric/bin/doctl completion bash)

# added by Miniconda3 4.3.11 installer
export PATH="/home/eric/miniconda3/bin:$PATH"

# ruby
export GEM_HOME=$HOME/.gem
export PATH=$PATH:$HOME/.gem/bin

export YUBIKEYGPG=3C6DEDE3201FAE175DFDAC2809E71555D5F132CC
