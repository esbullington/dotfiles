Config { 
	   font = "xft:inconsolatalgc:pixelsize=12:antialias=true"
   , bgColor =      "black"
   , fgColor =      "#646464"
   , position =     Top 
   , border =       BottomB
   , borderColor =  "#646464"
   , lowerOnStart = True
   , commands = [ Run Cpu ["-L","15","-H","50","--normal","green","--high","red"] 10
								, Run Date "<fc=#ABABAB>%F (%a) %T</fc>" "date" 10
--                , Run Volume "default" "Speaker" [] 10
								, Run BatteryP ["bq27500-0"] 
                    [
                      "-t", "<acstatus>" ,
                      "-L", "10", "-H", "80" , "-l", "red", "-h", 
                      "green" , "--", "-O", "Charging", "-o", "Battery: <left>%"
                     ] 10
                , Run StdinReader
                ]
   , sepChar ="%" 
   , alignSep = "}{"
   , template = "%StdinReader% }{ %battery% |  %cpu% | %date%"
}
